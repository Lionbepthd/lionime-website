{
  "version": 2,
  "name": "lionime",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://www.sankavollerei.com/anime/",
    "NEXT_PUBLIC_SITE_NAME": "Lionime"
  }
}
